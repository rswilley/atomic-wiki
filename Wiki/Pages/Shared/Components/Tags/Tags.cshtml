@model List<TagsList.TagsModel>

@foreach (var tag in Model)
{
    <a asp-page="/Tags/List" asp-route-nameEncoded="@tag.NameEncoded" class="badge bg-light text-dark border">@tag.Slug</a>
}
<a asp-page="/Tags/List"  asp-route-nameEncoded="untagged" class="badge bg-light text-dark border">untagged</a>
